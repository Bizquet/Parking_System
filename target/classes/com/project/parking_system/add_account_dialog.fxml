<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.project.parking_system.controllers.admin.AddAccountDialogController">
    <headerText>
        Input Username and Password for teller
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Username" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="txtUname" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Password" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <PasswordField fx:id="passField" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Confirm" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <PasswordField fx:id="passFieldConfirm" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        </GridPane>
    </content>

</DialogPane>
